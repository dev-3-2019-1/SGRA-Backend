<!DOCTYPE html>
<html>
  <head>
    <title>User List</title>
    <link rel='stylesheet' href='/public/stylesheets/style.css' type="text/css"/>
  </head>
  <body>
    <h1>User List</h1>
    <% if (userlist && userlist.length) { %>
      <ul>
        <% userlist.forEach((item) => { %>
          <li>
            <a class="test" href="/users/<%= item._id %>/edit"><%= item.username %></a>
            <a href="/users/<%= item._id %>/delete">Delete</a>
          </li>
        <% }) %>
      </ul>
    <% } %>
  </body>
</html>